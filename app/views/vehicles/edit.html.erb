<%= simple_form_for(@vehicle) do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :real_consumption %>
  <%= f.submit %>
<% end %>


<%= link_to "Delete",
  vehicle_path,
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>
<%= link_to "Back to Vehicles", vehicles_path,  class: "btn btn-flat" %>
